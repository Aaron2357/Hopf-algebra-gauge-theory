                RowBox[{"Diagonals", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], ",", "basis", ",", 
                "\[Omega]"}], "]"}]}], ";", "\[IndentingNewLine]", "\t", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"matString", ",", 
               RowBox[{"PrintPower", "[", 
                RowBox[{"\"\<\[Sigma]\>\"", ",", "exp"}], "]"}]}], "]"}], ";",
              "\[IndentingNewLine]", "\t", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"mat", ",", 
               RowBox[{"MatrixPower", "[", 
                RowBox[{"\[Sigma]1", ",", "exp"}], "]"}]}], "]"}], ";", 
             "\[IndentingNewLine]", "\t", 
             RowBox[{"tempMatrix", "=", 
              RowBox[{"Diagonals", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}]}], ",", "\[IndentingNewLine]",
             "\t", 
