Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Rep", "[", 
     RowBox[{"incoming_", ",", "cilia_", ",", "basis_", ",", "edges_"}], 
     "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "\[Chi]", ",", "\[Chi]perm", ",", "productTable", ",", "basisEdges", 
        ",", "reps", ",", "matrix", ",", "incomingperm"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[Chi]perm", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Permute", "[", 
            RowBox[{
             RowBox[{"IntegerDigits", "[", 
              RowBox[{"k", ",", "basis", ",", "edges"}], "]"}], ",", 
             RowBox[{"CyclicGroup", "[", "edges", "]"}]}], "]"}], "[", 
           RowBox[{"[", "cilia", "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{
             RowBox[{"basis", "^", "edges"}], "-", "1"}]}], "}"}]}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"\[Chi]", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Permute", "[", 
            RowBox[{
             RowBox[{"IntegerDigits", "[", 
              RowBox[{"k", ",", "basis", ",", "edges"}], "]"}], ",", 
             RowBox[{"CyclicGroup", "[", "edges", "]"}]}], "]"}], "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{
             RowBox[{"basis", "^", "edges"}], "-", "1"}]}], "}"}]}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"incomingperm", "=", 
        RowBox[{
         RowBox[{"Permute", "[", 
          RowBox[{"incoming", ",", 
           RowBox[{"CyclicGroup", "[", "edges", "]"}]}], "]"}], "[", 
         RowBox[{"[", "cilia", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"productTable", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"coeff", "[", 
           RowBox[{
            RowBox[{"\[Chi]perm", "[", 
             RowBox[{"[", "i", "]"}], "]"}], ",", 
            RowBox[{"\[Chi]perm", "[", 
             RowBox[{"[", "j", "]"}], "]"}], ",", "edges", ",", 
            "incomingperm", ",", "basis"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", 
            RowBox[{"Length", "[", "\[Chi]", "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", 
            RowBox[{"Length", "[", "\[Chi]", "]"}]}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"basisEdges", "=", 
        RowBox[{"IdentityMatrix", "[", "edges", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"reps", "=", 
        RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"matrix", "=", 
           RowBox[{"RegularRep", "[", 
            RowBox[{"b", ",", "productTable", ",", "\[Chi]", ",", "basis"}], 
            "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"reps", ",", 
            RowBox[{"FindTensorOne", "[", 
             RowBox[{"matrix", ",", "basis", ",", "edges"}], "]"}]}], "]"}]}],
          ",", 
         RowBox[{"{", 
          RowBox[{"b", ",", "basisEdges"}], "}"}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "reps", "]"}]}]}], "\[IndentingNewLine]", 
     "]"}]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"FindTensorOne", "[", 
     RowBox[{"matrix_", ",", "basis_", ",", "edges_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "exp", ",", "mat", ",", "matString", ",", "index", ",", "tempMatrix", 
        ",", "LowerDiag", ",", "Diagonals", ",", "\[Sigma]1", ",", "\[Tau]1", 
        ",", "\[Omega]", ",", "zeroM"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[Omega]", "=", 
        RowBox[{"Exp", "[", 
         RowBox[{"2", "Pi", "*", 
          RowBox[{"I", "/", "basis"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       
       RowBox[{"\[Tau]1", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"KroneckerDelta", "[", 
           RowBox[{"j", ",", 
            RowBox[{"i", "+", "1"}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "basis"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", "basis"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"\[Tau]1", "[", 
          RowBox[{"[", "basis", "]"}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}], "=", "1"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"\[Sigma]1", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"\[Omega]", "^", 
            RowBox[{"(", 
             RowBox[{"i", "-", "1"}], ")"}]}], 
           RowBox[{"KroneckerDelta", "[", 
            RowBox[{"j", ",", "i"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "basis"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", "basis"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"tempMatrix", "=", "matrix"}], ";", "\[IndentingNewLine]", 
       RowBox[{"matString", "=", 
        RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mat", "=", 
        RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"index", "=", 
           RowBox[{"basis", "^", 
            RowBox[{"(", 
             RowBox[{"edges", "-", "k"}], ")"}]}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"Diagonals", "=", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"tempMatrix", "[", 
              RowBox[{"[", 
               RowBox[{
                RowBox[{"j", ";;", 
                 RowBox[{"j", "+", "index", "-", "1"}]}], ",", 
                RowBox[{"j", ";;", 
                 RowBox[{"j", "+", "index", "-", "1"}]}]}], "]"}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", "1", ",", 
               RowBox[{"basis", "*", "index"}], ",", "index"}], "}"}]}], 
            "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"LowerDiag", "=", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"tempMatrix", "[", 
              RowBox[{"[", 
               RowBox[{
                RowBox[{
                 RowBox[{"j", "+", "index"}], ";;", 
                 RowBox[{"j", "+", 
                  RowBox[{"2", "index"}], "-", "1"}]}], ",", 
                RowBox[{"j", ";;", 
                 RowBox[{"j", "+", "index", "-", "1"}]}]}], "]"}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", "1", ",", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"basis", "-", "1"}], ")"}], "*", "index"}], ",", 
               "index"}], "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
          "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"basis", "\[Equal]", "2"}], ",", "\[IndentingNewLine]", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"LowerDiag", ",", 
              RowBox[{"tempMatrix", "[", 
               RowBox[{"[", 
                RowBox[{
                 RowBox[{"1", ";;", "index"}], ",", 
                 RowBox[{
                  RowBox[{
                   RowBox[{
                    RowBox[{"(", 
                    RowBox[{"basis", "-", "1"}], ")"}], "*", "index"}], "+", 
                   "1"}], ";;", 
                  RowBox[{"basis", "*", "index"}]}]}], "]"}], "]"}]}], 
             "]"}]}], "\[IndentingNewLine]", "]"}], ";", 
          "\[IndentingNewLine]", "\[IndentingNewLine]", 
          RowBox[{"zeroM", "=", 
           RowBox[{"ConstantArray", "[", 
            RowBox[{"0", ",", 
             RowBox[{"{", 
              RowBox[{"index", ",", "index"}], "}"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"If", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"Diagonals", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "\[NotEqual]", "zeroM"}], ",", 
